<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-4.3.xsd
		http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-4.3.xsd">

	<!-- SrgingPrinter 와 ConsolePrinter를 bea 등록 -->
	<bean id="sPrinter" class="myspring.di.xml.StringPrinter"></bean>
	<bean id="cPrinter" class="myspring.di.xml.ConsolePrinter"></bean>
	
	<bean id="hello" class="myspring.di.xml.Hello" scope="prototypes">
		<property name="name" value="황인용" />
		<property name="printer" ref="sPrinter" />
	</bean>
</beans>
